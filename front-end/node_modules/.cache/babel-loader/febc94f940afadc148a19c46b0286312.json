ba(23,198,113,0.1)',\n          borderColor: 'rgb(23,198,113)',\n          data: [1, 2, 3, 3, 3, 4, 4],\n        }],\n      }, {\n        label: 'Comments',\n        value: '8,147',\n        percentage: '3.8%',\n        increase: false,\n        decrease: true,\n        labels: ['Label', 'Label', 'Label', 'Label', 'Label', 'Label'],\n        datasets: [{\n          label: 'Today',\n          fill: 'start',\n          borderWidth: 1.5,\n          backgroundColor: 'rgba(255,180,0,0.1)',\n          borderColor: 'rgb(255,180,0)',\n          data: [2, 3, 3, 3, 4, 3, 3],\n        }],\n      }, {\n        label: 'New Customers',\n        value: '29',\n        percentage: '2.71%',\n        increase: false,\n        decrease: true,\n        labels: ['Label', 'Label', 'Label', 'Label', 'Label', 'Label'],\n        datasets: [{\n          label: 'Today',\n          fill: 'start',\n          borderWidth: 1.5,\n          backgroundColor: 'rgba(255,65,105,0.1)',\n          borderColor: 'rgb(255,65,105)',\n          data: [1, 7, 1, 3, 1, 4, 8],\n        }],\n      }, {\n        label: 'Subscribers',\n        value: '17,281',\n        percentage: '2.4%',\n        increase: false,\n        decrease: true,\n        labels: ['Label', 'Label', 'Label', 'Label', 'Label', 'Label'],\n        datasets: [{\n          label: 'Today',\n          fill: 'start',\n          borderWidth: 1.5,\n          backgroundColor: 'rgb(0,123,255,0.1)',\n          borderColor: 'rgb(0,123,255)',\n          data: [3, 2, 3, 2, 4, 5, 4],\n        }],\n      }];\n    },\n  },\n};\n</script>\n\n"],"sourceRoot":"src/views"}]}